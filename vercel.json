{
    "version": 2,
    "name": "ai-support-center",
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://your-api-url.railway.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "apps/web/$1"
        }
    ],
    "env": {
        "VITE_API_URL": "@vite_api_url"
    }
}