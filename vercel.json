{
    "version": 2,
    "name": "ai-support-center",
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://api-production-6c70.up.railway.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "apps/web/$1"
        }
    ]
}